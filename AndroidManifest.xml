<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.home.ncapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.home.ncapp.MAINACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NOOParameter"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.home.ncapp.NOOPARAMATER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NOOParalog"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.home.ncapp.NOOPARALOG" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NOOCustoOview"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.home.ncapp.NOOCUSTOOVIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>                 
    </application>
</manifest>
